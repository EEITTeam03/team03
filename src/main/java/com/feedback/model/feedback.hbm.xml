<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.feedback.model.FeedbackVO" table="feedback">
	<id name="feedbackNo" column="feedbackNo"><generator class="identity"/></id>
	<property name="feedbackDate" column="feedbackDate" />
	<property name="memberName" column="memberName" />
	<property name="memberNo" column="memberNo" />
	<property name="feedback" column="feedback" />
	<property name="reply" column="reply" />
	<property name="readState" column="readState" />
	</class>
</hibernate-mapping>